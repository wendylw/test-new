{
  "name": "beep",
  "version": "0.1.0",
  "private": true,
  "proxy": "https://storehubyougaga.beep.test12.shub.us",
  "engines": {
    "node": "12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run test:coverage"
    }
  },
  "lint-staged": {
    "src/**/*.(js|jsx|scss)": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 120,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "dependencies": {
    "@sentry/apm": "^5.19.2",
    "@sentry/integrations": "^5.19.2",
    "@sentry/react": "^5.19.2",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.4.0",
    "apollo-boost": "^0.3.1",
    "graphql": "^14.2.1",
    "hoist-non-react-statics": "^3.3.2",
    "i18next": "^19.0.2",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.15",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "qrcode-decoder": "^0.1.1",
    "qs": "^6.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.2.7",
    "react-infinite-scroller": "^1.2.4",
    "react-lazyload": "^2.6.5",
    "react-otp-input": "^0.3.3",
    "react-phone-number-input": "^2.3.13",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "smoothscroll-polyfill": "^0.4.4",
    "source-map-explorer": "^2.3.1",
    "swipejs": "^2.2.14",
    "swiper": "^6.1.2",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "build-sw": "node ./src/sw-build.js",
    "build-i18n": "node ./build-i18n-files.js",
    "clean-cra-sw": "rm -f build/precache-manifest.*.js && rm -f build/service-worker.js",
    "build": "react-app-rewired build && npm run build-sw && npm run clean-cra-sw && npm run build-i18n && npm run build:sourcemap && npm run move-to-backend",
    "prepush": "sh ../git_hooks/copy",
    "move-to-backend": "mkdir -p ../backend/client/build && cp -pr build/* ../backend/client/build/",
    "test": "react-app-rewired test",
    "test:watch": "react-app-rewired test --watchAll",
    "test:coverage": "react-app-rewired test --coverage --watchAll=false",
    "test:debug": "react-app-rewired --inspect-brk test --runInBand --no-cache",
    "eject": "react-app-rewired eject",
    "deploy": "deploy-aws-s3-cloudfront --source ./build --cache-control index.html:no-cache static/**/*:max-age=2592000 i18n/**/*:max-age=2592000",
    "i18n-sync": "i18n-sync",
    "build:sourcemap": "node scripts/build-sourcemap --serve-sourcemap-url http://localhost:8080",
    "serve:sourcemap": "node scripts/serve-sourcemap --port 8080",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-unused-expressions": [
        "off"
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@sentry/cli": "^1.55.0",
    "@sentry/webpack-plugin": "^1.12.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.0",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "aws-sdk": "^2.706.0",
    "customize-cra": "^0.9.1",
    "debug": "^4.1.1",
    "deploy-aws-s3-cloudfront": "^3.2.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "husky": "^3.1.0",
    "i18n-sync": "^1.2.1",
    "jest-fetch-mock": "^3.0.1",
    "less-loader": "^6.1.1",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "proxy-agent": "^3.1.1",
    "react-app-rewired": "^2.1.5",
    "react-docgen-typescript-plugin": "^0.5.0",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.4",
    "workbox-build": "^4.3.0",
    "yargs": "^15.3.1"
  }
}
