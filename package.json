{
  "name": "beep",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://nike.storehub.local:7000",
  "dependencies": {
    "apollo-boost": "^0.3.1",
    "graphql": "^14.2.1",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "query-string": "^6.5.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.4",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-sw": "node ./src/sw-build.js",
    "clean-cra-sw": "rm -f build/precache-manifest.*.js && rm -f build/service-worker.js",
    "build": "react-scripts build && npm run build-sw && npm run clean-cra-sw && npm run move-to-backend",
    "move-to-backend": "mkdir -p ../backend/client/build && cp -pr build/* ../backend/client/build/",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "workbox-build": "^4.3.0"
  }
}
